const repeatString = function (string, repeatTimes) {
  let output = "";
  if (repeatTimes < 0) return "ERROR";
  for (let i = 0; i < repeatTimes; i++) {
    output += string;
  }
  return output;
};

// result = repeatString("hey", 3);
// console.log(result);

// Do not edit below this line
module.exports = repeatString;
